{
    "version": 2,
    "routes": [{
            "src": "/",
            "dest": "/api/index.js?username=gigamaster"
        },
        {
            "src": "/user/(?<username>[^/]*)",
            "dest": "/api/index.js?username=$username"
        },
        {
            "src": "/u/(?<username>[^/]*)",
            "dest": "/api/index.js?username=$username"
        }
    ]
}
